@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - WINHMS Application</title>
    <link href="~/winhmsfav.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />   
    @*<link href="~/Content/Style/Dtbootstrap.css" rel="stylesheet" />*@
      <link href="~/Content/bootstrap.css" rel="stylesheet" />  
    <link href="~/Content/WinCommon.css" rel="stylesheet" />
    <link href="~/Content/FOstyle.css" rel="stylesheet" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/plugins/css")
</head>
<body style="overflow:hidden;">
    <div id="cssmenu" style=" top: 0px;">
        <div class="homebtnss" style="width:40px; margin-top:2px;">

            <a id="hlnkHome" href="/CM/TEHome.aspx" style="float: right;  "><img src="../../Images/home.png" width="18" height="18" alt="Home" style="visibility:hidden;"></a>
        </div>
        <ul id="ulMenus" class="leftwidth"></ul>
    </div>
    <div class="TopHdrName">
        <div class="compnys">

        </div>
        <div class="usr">
            <img src="../../Images/User.png" style="width:15px; visibility:hidden; margin-top:10px; height:15px;">
        </div>
        <div class="dashbrd">
            <a id="hlkDB" href="../" style="float: right; padding-right:5px;"><img src="../../Images/dashboard.png" width="18" height="18" alt="Home"></a>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryval")
    @RenderBody()

    @RenderSection("scripts", required: false)
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/modernizr.min.js"></script>
    <script src="~/Content/js/detect.js"></script>
    <script src="~/Content/js/fastclick.js"></script>
    <script src="~/Content/js/jquery.slimscroll.js"></script>
    <script src="~/Content/js/jquery.blockUI.js"></script>
    <script src="~/Content/js/waves.js"></script>
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/jquery.nicescroll.js"></script>
    <script src="~/Content/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/plugins/bootstrap-sweetalert/sweet-alert.min.js"></script>
    <script src="~/Content/pages/sweet-alert.init.js"></script>
    <script src="~/Content/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/plugins/datatables/dataTables.bootstrap.js"></script>
    <script src="~/Content/plugins/datatables/dataTables.buttons.min.js"></script>
