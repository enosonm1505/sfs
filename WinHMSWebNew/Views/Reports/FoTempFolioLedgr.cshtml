                            if (vAmt != null && vAmt != undefined) {
                                var vAmt1 = vAmt.replace(",", ""); vAmt1 = vAmt1.replace(",", ""); vAmt1 = vAmt1.replace(",", ""); vAmt1 = vAmt1.replace(",", "");
                                vAmt1 = vAmt1.replace(",", ""); vAmt1 = vAmt1.replace(",", "");
