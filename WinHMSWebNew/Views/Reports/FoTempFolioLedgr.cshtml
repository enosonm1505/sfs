                            { id: "ixBal", header: "Balance", width: 100, css: { 'text-align': 'right ! important' } , exportType: "number", exportFormat: "#,##0.00",   },
                            { id: "ixRefTy", header: "Settle By", width: 90, css: { 'text-align': 'left ! important' },hidden:true   },
